<!-- views/pages/index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <% include partials/head %>
</head>
<body class="container">

    <header>
        <% include partials/header %>
    </header>

    <main>
        <div class="jumbotron">
            <h1><%= title %></h1>
            <p>Welcome to booksmarks category</p>
        </div>

    </main>
<div class="container">
<div class="row">
<div class="col-md-12">
<div id="error" class="" role="alert"></div>
<form method="post" action="/category/<%=category._id %>/edit" enctype='application/x-www-form-urlencoded' id="category_new">
  <div class="form-group">
    <label for="category">Category</label>
    <input type="text" class="form-control" value="<%=category.category %>" id="category" name="category" placeholder="Category">
  </div>
 <input type="hidden" value="PUT" name='_method'> 
<button type="submit" class="btn btn-primary">Submit</button>
</form>

</div>
</div>

</div>

    <footer>
        <% include partials/footer %>
    </footer>
    
</body>
</html>