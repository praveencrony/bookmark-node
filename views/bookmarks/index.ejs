<!-- views/pages/index.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <% include partials/head %>
</head>
<body class="container">

    <header>
        <% include partials/header %>
    </header>

    <main>
        <div class="jumbotron">
            <h1>Bookmarks</h1>
            <p>Add Your Bookmarks</p>
        </div>

    </main>
<div class="container">
<div class="row">
<div class="col-md-12">
<% bookmarks.forEach(function(msgObj,index){ %>
<div class="col-md-3 text-center bookmarkGrid">
<h2><%= msgObj.category %></h2>
<% msgObj.bookmarks.forEach(function(msgObj,index){ %>
<li><a href="<%=msgObj.url%>" target="_blank"><%=msgObj.name%></a></li>
<% }) %>
</div>
<div class="col-md-1">&nbsp;</div>
  <% }) %>
</div>

</div>

</div>

<footer>
	<% include partials/footer %>
</footer>
    
</body>
</html>